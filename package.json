  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/jest-css-modules"
    }
  },
  "scripts": {
    "start": "nodemon -e js,jsx --ignore dist --exec babel-node ./src/server/bin/www",
    "start:wds": "webpack -w",
    "prod:start": "cross-env NODE_ENV=production && node ./dist/server/bin/www",
    "prod:build": "rimraf dist && babel src -d dist --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
    "lint": "./node_modules/.bin/eslint src/",
    "test": "npm run lint && jest src/* --coverage",
    "prepush": "npm test"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-2"
    ]
  },
  "dependencies": {
    "axios": "^0.16.2",
