{
  "name": "Kiddo",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/VolkRiot/Kiddo.git",
  "author": "",
  "license": "",
  "jest": {
    "setupFiles": [
      "<rootDir>/src/test-setup.js"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/jest-css-modules"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "scripts": {
    "start": "nodemon -e js,jsx --ignore dist --exec babel-node ./src/server/bin/www",
    "start:wds": "webpack -w",
    "prod:start": "cross-env NODE_ENV=production && node ./dist/server/bin/www",
    "prod:build": "rimraf dist && babel src -d dist --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
    "lint": "eslint src webpack.config.js --ext .js,.jsx",
    "lint:fix": "eslint src webpack.config.js --ext .js,.jsx --fix",
    "test": "npm run lint && jest src/* --env=jsdom",
    "test:shallow": "npm run lint",
    "test:rich": "npm run lint && jest src/* --coverage",
    "prepush": "npm run test:shallow",
    "seed:all": "npm run seed:users && npm run seed:kids",
    "seed:users": "mongoimport --db kiddo_db --collection users --drop --file seed/userSeed.json",
    "seed:kids": "mongoimport --db kiddo_db --collection kids --drop --file seed/kidSeed.json"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-2"
    ]
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "cross-env": "^5.0.1",
    "csshake": "^1.5.2",
    "debug": "~2.6.3",
    "dotenv": "^4.0.0",
    "express": "~4.15.2",
    "express-session": "^1.15.3",
    "filestack-js": "^0.7.1",
    "find-cache-dir": "^1.0.0",
    "fullcalendar": "^3.4.0",
    "google-auth-library": "^0.10.0",
    "google-calendar": "^1.3.2",
    "google-fonts": "^1.0.0",
    "googleapis": "^20.0.1",
    "jquery": "^3.2.1",
    "jsdom": "^11.1.0",
    "jstz": "^1.0.9",
    "moment": "^2.18.1",
    "mongoose": "^4.10.8",
    "morgan": "~1.8.1",
    "passport": "^0.3.2",
    "passport-google-oauth20": "^1.0.0",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.6.1",
    "react-favicon": "0.0.9",
    "react-modal": "^2.2.2",
    "react-router": "^4.1.1",
    "react-router-bootstrap": "^0.24.2",
    "react-router-dom": "^4.1.2",
    "react-slick": "^0.14.11",
    "semantic-ui-react": "^0.71.1",
    "serve-favicon": "~2.4.2",
    "slick-carousel": "^1.6.0",
    "vex-dialog": "^1.0.7",
    "vex-js": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "jest-css-modules": "^1.1.0",
    "nodemon": "^1.11.0",
    "react-test-renderer": "^15.6.1",
    "style-loader": "^0.18.2",
    "supertest": "^3.0.0",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.5.0"
  }
}
